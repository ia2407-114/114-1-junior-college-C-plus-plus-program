#include <stdio.h>

struct student
{
	int id, computer, math;
	char name[20];
	float average;
};

int main()
{
	int key, low, high, middle, result;
	struct student stu[10]
	{	{11, 64, 70, "Danny", 0.0},
		{12, 92, 80, "Mary", 0.0},
		{13, 78, 75, "Jimmy", 0.0},
		{14, 85, 90, "Peter", 0.0},
		{15, 90, 85, "Sue", 0.0},
		{16, 85, 70, "John", 0.0},
		{17, 76, 82, "Hearts", 0.0},
		{18, 95, 92, "Dumb", 0.0},
		{19, 88, 88, "Clubs", 0.0},
		{20, 85, 70, "Spades", 0.0}
	};
	for (int i = 0; i < 9; i++)
	{
		stu[i].average = (stu[i].computer + stu[i].math) / 2.0;
	}
	printf("姓名\t學號\t計概\t數學\t平均\n");
	for (int i = 0; i < 9; i++)
	{
		printf("%s\t%d\t%d\t%d\t%f\n", stu[i].name, stu[i].id, stu[i].computer, stu[i].math, stu[i].average);

	}
}
