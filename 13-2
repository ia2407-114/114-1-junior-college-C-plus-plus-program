#include <stdio.h>
#define N 20
int main(void)
{
    int id[N] = {12,13,14,15,16,17,19,21,22,23,24,26,27,28,29,31,32,33,36,37};
    int score[N] = {66,67,66,70,73,74,75,75,62,32,44,46,27,12,80,63,66,90,11,66};
    for (int i = 0; i < N - 1; ++i) {
        for (int j = 0; j < N - 1 - i; ++j) {
            if (score[j] > score[j + 1]) {
                int tmp = score[j];
                score[j] = score[j + 1];
                score[j + 1] = tmp;

                tmp = id[j];
                id[j] = id[j + 1];
                id[j + 1] = tmp;
            }
        }
    }
    printf("學號    成績\n");
    for (int i = 0; i < N; ++i) {
        printf("%d\t%d\n", id[i], score[i]);
    }

    return 0;
}
