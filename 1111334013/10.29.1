#include <stdio.h>


int main() {
    float height=0, weight=0, bmi;

    printf("請輸入身高(公尺):");
    scanf_s("%f", &height);
    printf("請輸入體重(公斤):");
    scanf_s("%f", &weight);

    bmi = calcbmi(height, weight);

    printf("您的BMI為: %.2f\n", bmi);


    return 0;
}float calcbmi(float height, float weight) {
    float bmi;
    bmi = weight / (height * height);
    return bmi;
}
